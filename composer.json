{
    "name": "fuguevit/douban",
    "description": "Douban api laravel implementation",
    "keywords": ["douban", "laravel"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.0",
        "illuminate/config": "^5.3.0",
        "illuminate/support": "^5.3.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench": "3.3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Fuguevit\\Douban\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fuguevit\\Douban\\Test\\": "tests/"
        }
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
